<a class="btn bordo modal-mount" data-link="<%= data.adicionar_link %>" data-href="#modalinfo2">Adicionar Novo Registro</a>
<a class="btn bordo">Gerar Relatório</a>

<table class="centered highlight tabela_loc_processos tabela_filtrada cell-border" id="tabela_interna">
    <thead>
      <tr>
        <th>
          Selecionar
        </th>
        <th>
          Descrição Geral
        </th>
        <th>
          Ações
        </th>
      </tr>
    </thead>
    <tbody>
<%
	if (typeof data.dados != 'undefined' && data.dados.length > 0) {
		for (var i = data.dados.length - 1; i >= 0; i--) {
	 %>
	 <tr>
	 	<td><a class="btn bordo selecionar-item-modal" data-id="<%= data.dados[i].id%>" data-anchorId="" data-descricao="<%= data.dados[i].descricao %>">Selecionar</a></td>
	 	<td><%= data.dados[i].descricao %></td>
	 	<td>     
	 		<a href="<%= data.dados[i].id %>" class="ajax-load" title="Editar Registro">
	 			<i class="fa fa-pencil yellow-text text-darken-3" aria-hidden="true"></i>
	 		</a>
      <a href="">
      	<i class="fa fa-times-circle red-text modal-remover-mount" data-href="#modaldeletar" data-id="<%= data.dados[i].id %>" data-texto="Você tem certeza que quer deletar '<%= data.dados[i].descricao %>'? " data-to="/sistema/processos/desativar" data-back="/sistema/processos" aria-hidden="true" title="Excluir Registro"></i>
      </a>
    </td>
	 </tr>

	<%}}else{ %>
	<tr>Não há Dados</tr>

	<% } %>