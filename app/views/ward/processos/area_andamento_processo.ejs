		<div class="col s12">
			<h5 class="center">Andamentos do Processo</h5>
			
			<div class="col s12 valign-wrapper">
				<div class="input-field input-group-control col s12 m6">
					<label for="andamento_data">Data</label>
					<input class="data_sem_hora data_hoje" type="text" name="andamento_data" maxlength="10" value="<%= data.tempo[0].hoje%>">
				</div>
				<div class="select-field input-group-control col s12 m6">
					<label>Tipo do Andamento</label>
					<select class="browser-default" name="andamento_tipo">
						<option value="0">Jurídico</option>
						<option value="1">Administrativo</option>
						<option value="2">Extra Especial</option>
					</select>
				</div>
				<div class="input-field input-group-control col s12">
					<label for="andamento_descricao">Descrição</label>
					<textarea name="andamento_descricao"></textarea>
				</div>				
			</div>

				<button data-href="/sistema/processos/andamentos_cadastrar" class="btn waves-effect waves-light bordo ajax-submit-no-back right">Salvar Novo Andamento</button>



			</div>
			
			<div class="col s12">
				<div class="card white padding-a-15">
					<table id="tabela_andamentos" class="cell-border">
						<thead>
							<tr>
								<th>Data</th>
								<th>Descrição</th>
								<th>Usuário</th>
								<th>Tipo do Andamento</th>
								<th>Data Cadastro</th>
							</tr>
						</thead>							
						<tbody>		

							<%
								for (var k = data.andamentos.length - 1; k >= 0; k--){
									%>
									<tr>
										<td><%= data.andamentos[k].data %></td>
										<td><%= data.andamentos[k].descricao %></td>
										<td><%= data.andamentos[k].usuario %></td>
										<td>
											
											<% switch(data.andamentos[k].tipo){
												case 0:%>
												Juridico
												<% break;
												case 1:%>
												Administrativo
												<%break;
												case 2:%>
												Extra Especial
												<%break;}%>

										</td>
										<td><%= data.andamentos[k].data_cadastro %></td>
									</tr>			

									<%}%>
								</tbody>
							</table>
						</div>
					</div>