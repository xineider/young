<a class="btn bordo ajax-load" data-link="<%= data.adicionar_link %>" data-href="#modalinfo2">Adicionar Novo Registro</a>
<a class="btn bordo">Gerar Relatório</a>

<form method="POST" action="" enctype="multipart/form-data">
<table class="centered highlight tabela_loc_processos tabela_filtrada" id="tabela_interna">
    <thead>
      <tr>
        <th>
          Selecionar
        </th>
        <th>
          Descrição Geral
        </th>
      </tr>
    </thead>
    <tbody>
<%
	if (typeof data.dados != 'undefined' && data.dados.length > 0) {
		for (var i = data.dados.length - 1; i >= 0; i--) {
	 %>
	 <tr>
	 	<td>
      <a class="btn bordo ajax-submit-no-form" data-href='<%= data.cadastrar_item %>' data-id='<%=data.id_processo%>' data-id-cliente='<%=data.dados[i].id%>'>Selecionar</a></td>
	 	<td><%= data.dados[i].descricao %></td>
	 </tr>

	<%}}else{ %>
	<tr>Não há Dados</tr>

	<% } %>

</tbody>
</table>
</form>