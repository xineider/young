<a class="btn waves-effect waves-light golden darken-2 bordo-text gerar-relatorio" data-nome-provisorio="<%= data.nome_relatorio%>" data-container-relatorio="#tabela_interna">
  <i class="fa fa-file-pdf-o" aria-hidden="true"></i>
  Gerar Relatório
</a>

<table class="centered highlight tabela_loc_processos tabela_filtrada cell-border" id="tabela_interna">
  <thead>
    <tr>
      <th>
        Selecionar
      </th>
      <th>
        Descrição Geral
      </th>
    </tr>
  </thead>
  <tbody>
    <%
    if (typeof data.dados != 'undefined' && data.dados.length > 0) {
      for (var i = data.dados.length - 1; i >= 0; i--) {
        %>
        <tr>
         <td>
          <a class="btn bordo selecionar-item-modal" data-id="<%= data.dados[i].id%>" data-anchorId="" data-descricao="<%= data.dados[i].descricao %>">Selecionar</a>
        </td>
        <td><%= data.dados[i].descricao %></td>
     </tr>

     <%}}else{ %>
     <tr>Não há Dados</tr>

     <% } %>