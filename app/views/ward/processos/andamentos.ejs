    <form method="POST" action="" enctype="multipart/form-data">

      <h5 class="center">Cadastrar Andamento</h5>

      <input type="hidden" name="id" value="<%= data.id_processo %>" id="id_processo">
      
      <div class="col s12 valign-wrapper">
        <div class="input-field input-group-control col s12 m6">
          <label for="andamento_data">Data</label>
          <input class="data_sem_hora data_hoje" type="text" name="andamento_data" maxlength="10" value="<%= data.tempo[0].hoje%>">
        </div>
        <div class="select-field input-group-control col s12 m6">
          <label>Tipo do Andamento</label>
          <select class="browser-default" name="andamento_tipo">
            <option value="0">Jurídico</option>
            <option value="1">Administrativo</option>
            <option value="2">Financeiro</option>
            <option value="3">Outro</option>
          </select>
        </div>
        <div class="input-field input-group-control col s12">
          <label for="andamento_descricao">Descrição</label>
          <textarea name="andamento_descricao" class="validate"></textarea>
        </div>        
      </div>
      <div class="col s12">
        <button data-href="/sistema/processos/andamentos_cadastrar" class="btn waves-effect waves-light bordo ajax-submit-container-reload right" data-container="#container_tabela_more_andamentos_reload">Cadastrar Novo Andamento</button>
      </div>

    </form>

    <div class="col s12">
      <div class="card white padding-a-15" id="container_tabela_more_andamentos_reload">
        <%- include('tabela_andamentos_only'); %>
      </div>

    </div>